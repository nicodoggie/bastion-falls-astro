---
title: <%= title %>
character:
  ddb: <%= character.ddb %>
  <%_ if (character.image) { _%>
  image:
    url: <%= character.image.url %>
    attribution: <%= character.image.attribution %>
    attribution_url: <%= character.image.attribution_url %>
  <%_ } _%>
  details:
    aliases:
      <%_ character.details.aliases.forEach(function(alias){ _%>
      - <%- alias %>
      <%_ }) _%>
    age: <%= character.details.age %>
    date_of_birth: <%= character.details.date_of_birth %>
    date_of_death: <%= character.details.date_of_death %>
    sex: <%= character.details.sex %>
    pronouns: <%= character.details.pronouns %>
    height: <%= character.details.height %>
    weight: <%= character.details.weight %>
    
tags:
  <%_ tags.forEach(function(tag){ _%>
  - <%- tag %>
  <%_ }) _%>
---

