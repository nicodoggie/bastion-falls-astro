          "phoneticRep": "li"
        },
        "etymology": {
          "@type": "etymon:Etymology",
          "etymon:protoform": "Proto-Hick *li",
          "etymon:note": "Ancient diminutive marker, preserved in many compounds",
          "etymon:soundChange": "stable form, triggers compound fusion"
        },
        "sense": [
          {
            "@type": "ontolex:LexicalSense",
            "definition": {
              "@value": "diminutive prefix: small, lesser, young",
              "@language": "en"
            },
