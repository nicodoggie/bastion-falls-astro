{
  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}/astro",
      "command": "../node_modules/.bin/astro dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "cwd": "${workspaceFolder}/cli",
      "command": "./node_modules/.bin/tsx --tsconfig src/tsconfig.json src/bin/cli.ts",
      "name": "Development CLI",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
